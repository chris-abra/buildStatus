{
  "build": "faaslang",
  "name": "Passion Sports/statusPage",
  "timeout": 30000,
  "env": [
    {
      "name": "LOGO_URL",
      "description": "Insert a link to your logo.",
      "defaultValue": "https://www.passionsports.ca/wp-content/uploads/2018/04/Official-Logo.png"
    },
    {
      "name": "LATENCY_THRESHOLD",
      "description": "Performance benchmark",
      "defaultValue": "350"
    },
    {
      "name": "MAIN_PAGE_URL",
      "description": "Link to your main page",
      "defaultValue": "https://passionsports.ca"
    }
  ]
}
